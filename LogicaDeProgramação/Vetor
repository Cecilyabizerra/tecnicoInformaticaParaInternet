programa
{
	
	funcao inicio()
	{
	real notas[12] 
	inteiro i 
	
	escreva("Digite suas notas:\n")
	para(i = 11; i >= 0  ; i--){
		
		leia(notas[i])
	}

	

	

	
		
	}
}
